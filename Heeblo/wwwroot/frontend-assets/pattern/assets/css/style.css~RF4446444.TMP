        font-size: 30px;
        line-height: 35px;
    }

    .home-main .main-form-center {
        width: 90%;
        top: 50%;
    }

    .main-form-center {
        width: 90%;
        top: 50%;
    }

    .upload-page #files {
        font-size: 12px;
    }


    .upload-page input[type=file]::file-selector-button {
        border: none;
        background: #fff;
        padding: 15px 30px 0px 0px;
        border-radius: 0px;
        color: #006480;
        font-size: 13px;
    }

    .thankyou-banner .upload.file img {
        max-width: 60px;
        padding: 70px 0px 20px;
    }

    .thankyou-page h2 {
        font-size: 30px;
        line-height: 30px;
    }

    .thankyou-page p {
        font-size: 14px;
        color: #fff;
        line-height: 50px;
        font-family: orkney medium;
    }

    .upload.file p {
        padding: 0;
        font-size: 20px;
    }

    .bottom-info {
        bottom: 0;
        margin: 100px 0px 0px 0px;
        top: auto;
    }


    .upload-btn label.button {
        padding: 12px 45px;
        margin-bottom: 0px;
        font-size: 12px;
    }

    .upload.file img {
        max-width: 60px;
        padding-bottom: 10px;
    }

    .upload-page .signup-line a {
        font-size: 15px;
    }

    .upload-page .signup-line {
        margin: 0px 0px 13px 0px;
    }

    .signup-line {
        margin: 20px 0px 13px 0px;
    }

    span.mb {
        font-size: 14px;
    }



    .form-info input {
        height: 50px;
        font-size: 14px;
    }

    .form-info button {
        padding: 14px 0;
        font-size: 14px;
    }

    .bottom-info {
        bottom: 30px;
    }

    .offset-3 {
        margin-left: 0;
    }

    .signup-line a {
        font-size: 12px;
    }

    .top-logo img {
        max-width: 80px;
    }

    .navbar-toggler:focus {
        text-decoration: none;
        outline: 0;
        box-shadow: none !important;
    }

    .navbar-nav {
        margin-top: 30px;
    }

    .footer__body {
        margin: 10px 0;
    }

    header ul.navbar-nav li {
        margin: 0px 0;
    }


    .banner__content img {
        max-width: 60%;
        display: block;
        margin: auto;
    }

    .home-page img {
        width: 50%;
        display: block;
        margin: auto;
    }

    .home-page p {
        font-size: 12px;
        line-height: 20px;
        padding: 15px 0;
        margin: 0;
    }

    .home-page button {
        font-size: 12px;
        width: 100%;
        padding: 15px 0px;
    }

    .bottom-info a {
        font-size: 7px;
    }

    .login-tittle h2 {
        font-size: 50px;
        text-transform: capitalize;
        color: #fff;
        font-weight: bold;
        margin: 0;
        line-height: 42px;
    }

    .social-media a {
        display: block;
        width: 100%;
        margin-bottom: 10px;
        font-size: 10px;
        padding: 4px 4px;
    }

    .welcome-screen {
        padding: 0;
    }

    .register-page .login-tittle h3 {
        font-size: 40px;
        line-height: 50px;
    }

    .login-tittle h3 {
        font-size: 20px;
    }
}


@media(min-width:768px) and (max-width:1024px) {
    .product-holder {
        width: 90%;
        height: 77%;
    }

    .product-side ul {
        margin: 20px 25px;
    }

    .product-side li {
        width: 70px;
        display: inline-block;
        height: auto;
    }

    .product-side {
        width: 100%;
        overflow: unset;
    }

    header a.nav-link:after {
        height: 34px;
    }

    a.theme-btn:after {
        height: 47px;
    }

    header ul.navbar-nav li {
        margin: 0px 0;
    }

    header a.nav-link {
        background: #ed1e26;
        border-radius: 50px;
        padding: 14px 30px !IMPORTANT;
        font-size: 11px;
        margin: 0px 15px;
    }


    a.theme-btn {
        padding: 20px 40px;
        margin-top: 40px;
        font-size: 17px;
    }

    .price-main {
        width: 32%;
        margin: 40px 0;
    }

    .pricing__body ul li {
        padding-left: 74px;
        font-size: 14px;
        line-height: 40px;
    }
}
