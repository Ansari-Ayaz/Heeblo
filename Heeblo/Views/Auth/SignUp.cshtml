@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <!-- ========== Meta Tags ========== -->
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="description" content="Stock Logistics: Powered by Matrixninjas" />
    <meta name="csrf-token" content="rZ3zX1dNEGz3R30Ek9IafBt5MJp31NkEnluQJfNR" />
    <meta name="google-signin-client_id" content="370069400410-srd922ua4fpqfkkfgjlnuddf0je47f9v.apps.googleusercontent.com">
    <!-- ========== Page Title ========== -->
    <title> Heeblo</title>
    <!-- ========== Favicon Icon ========== -->

    <link rel="shortcut icon" href="~/frontend-assets/pattern/assets/images/fav.png" type="image/x-icon" />
    <!-- Other css -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.css" />
    <link rel="stylesheet" href="~/frontend-assets/pattern/assets/css/animate.css" />
    <link rel="stylesheet" href="~/frontend-assets/pattern/assets/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/frontend-assets/pattern/assets/css/all.min.css">
    <link rel="stylesheet" href="~/frontend-assets/pattern/assets/css/icofont.min.css" />
    <link rel="stylesheet" href="~/frontend-assets/pattern/assets/css/swiper.min.css" />
    <link rel="stylesheet" href="~/frontend-assets/pattern/assets/css/style.css" />
    <link rel="stylesheet" href="~/frontend-assets/pattern/assets/css/rangeui.css" />


    <!-- ========== End Stylesheet ========== -->
    <script>
        function openNav() {
            document.getElementById("mySidenav").style.width = "350px";
        }

        function closeNav() {
            document.getElementById("mySidenav").style.width = "0";
        }
    </script>

</head>
<body>


    <div class="main-wraper ">
        <div class="preloader">
            <div class="preloader-inner">
                <div class="preloader-icon">
                    <span></span>
                    <span></span>
                </div>
            </div>
        </div>
        <header>
            <div class="row">
                <div class="container-fluid">
                    <nav class="navbar navbar-expand-lg">
                        <div class="container-fluid">
                            <a class="navbar-brand" href="#">
                                <div class="logo-nav">
                                    <img src="~/frontend-assets/pattern/assets/images//heeblo-logo.png" alt="Logo" />
                                </div>
                            </a>
                            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                                <span class="navbar-toggler-icon"> <i class="fa-solid fa-bars"></i></span>
                            </button>
                            <div class="collapse navbar-collapse" id="navbarNav">
                                <ul class="navbar-nav">
                                    <li class="nav-item">
                                        <a class="nav-link active" aria-current="page" href="/home">Home</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="http://localhost/customer/login"><span class="outer-border">log in   </span></a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="http://localhost/customer/register">Signup </a>
                                    </li>

                                </ul>
                            </div>
                        </div>
                    </nav>
                </div>
            </div>
        </header>


        <div class="content-main-wraper">

            <!-- Start Banner

               ============================================= -->



            <div class="banner register-page">
                <div class="container">
                    <div class="content-login">

                        <div class="row" style="align-items: center;">

                            <div class="top-logo">
                                <img src="~/frontend-assets/pattern/assets/images/heeblo-logo.png" alt="Logo" />
                            </div>
                            <div class="col-lg-6 col-md-6 main-form-center">
                                <div class="login-tittle mb-4 register-page">
                                    <h3> Sign Up</h3>
                                </div>
                                <div class="form-info wow fadeInDown" data-wow-duration="1.6s" style="visibility: visible; animation-duration: 1.6s; animation-name: fadeInDown;">
                                    <div class="col-md-12 btn-login">
                                        <button type="button" onclick="Google()">
                                            <span> <i class="fa-brands fa-google"></i> Google</span>
                                        </button>
                                        <button type="button" onclick="Email()">
                                            <span> <i class="fa-solid fa-envelope"></i> Email</span>
                                        </button>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="container-fluid bottom-info">

                    <div class="left-ft"><a href="mailto:admin@heeblo.com"><i class="fa-solid mailto:fa-envelope"></i>admin@heeblo.com</a></div>
                    <div class="right-ft">
                        <ul>
                            <li><a href="@Url.Action("termsandcondition","Auth")">Terms and Conditions </a></li>
                            <li><a href="@Url.Action("privacypolicy","Auth")">Privacy Policy</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>

        <footer class="footer">
            <div class="footer__top padding-tb">
                <div class="container-fluid">
                    <div class="row">

                        <div class="col-lg-3 col-sm-3 col-12">
                            <div class="logo wow fadeInUp" data-wow-delay=".5s">
                                <img src="~/frontend-assets/pattern/assets/images/logo-2.png" alt="Logo" />
                            </div>

                        </div>
                        <div class="col-lg-3 col-sm-3 col-12">
                            <div class="footer__body wow fadeInUp" data-wow-delay=".5s" style="visibility: visible; animation-delay: 0.5s;">
                                <h2> Menu</h2>
                                <ul>
                                    <li> <a href="#"> Home </a> </li>
                                    <li> <a href="#"> Login  </a> </li>
                                    <li><a href="#">  SignUp</a>  </li>

                                </ul>
                            </div>

                        </div>

                        <div class="col-lg-3 col-sm-3 col-12">
                            <div class="footer__body wow fadeInUp" data-wow-delay=".5s" style="visibility: visible; animation-delay: 0.5s;">
                                <h2> Important Links</h2>
                                <ul>
                                    <li> <a href="#"> Term & Condition  </a> </li>
                                    <li> <a href="#"> Privacy & Policy  </a> </li>


                                </ul>
                            </div>

                        </div>

                        <div class="col-lg-3 col-sm-3 col-12">
                            <div class="social-bottom wow fadeInUp" data-wow-delay=".5s" style="visibility: visible; animation-delay: 0.5s;">
                                <h2> Social Links</h2>
                                <ul>
                                    <li> <a href="#"> <i class="fa-brands fa-facebook-f"></i></a></li>
                                    <li> <a href="#"><i class="fa-brands fa-instagram"></i></a></li>
                                    <li> <a href="#"><i class="fa-brands fa-twitter"></i></a></li>
                                </ul>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <div class="footer__bottom wow fadeInUp" data-wow-delay=".5s">
                <div class="container-fluid">
                    <div class="row">
                        <p> © COPYRIGHT 2023 . ALL RIGHTS RESERVED.</p>
                    </div>
                </div>
            </div>

        </footer>



        <!-- ==========Footer Section Ends Here========== -->
        <!-- All Scripts -->

        <script src="~/frontend-assets/pattern/assets/js/jquery.js"></script>
        <script src="https://code.jquery.com/ui/1.13.2/jquery-ui.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.js"></script>
        <script src="~/frontend-assets/pattern/assets/js/bootstrap.bundle.min.js"></script>
        <script src="~/frontend-assets/pattern/assets/js/waypoints.min.js"></script>
        <script src="~/frontend-assets/pattern/assets/js/swiper.min.js"></script>
        <script src="~/frontend-assets/pattern/assets/js/wow.min.js"></script>
        <script src="~/frontend-assets/pattern/assets/js/lightcase.js"></script>
        <script src="~/frontend-assets/pattern/assets/js/isotope.pkgd.min.js"></script>
        <script src="~/frontend-assets/pattern/assets/js/functions.js"></script>


        <script>
            function Google() {
                window.location.href = '@Url.Action("SignInWithGoogle","Auth")'
            }
            function Email() {
                window.location.href = '@Url.Action("Register","Auth")'
            }
        </script>





    </div>

</body>
</html>
